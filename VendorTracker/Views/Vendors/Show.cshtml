@{
    Layout = "_Layout";
}

@using VendorTracker.Models;

<h1>Vendor's Orders:</h1>

<ol>
    @foreach (Order order in @Model["orders"])
    {
        <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Details</a></li>
    }
</ol>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>New Order</a></p>
<p><a href='/vendors'>Vendor List</a></p>
<p><a href='/'>Home</a></p>
